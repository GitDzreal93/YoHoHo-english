    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.30.1",
    "@capacitor/cli": "^6.0.0"
